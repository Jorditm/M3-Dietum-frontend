target;\n    // console.log(dietitian);\n\n    setPatient({ ...patient, gender: value });\n  };\n\n  const handleSmokeChange = (event) => {\n    const { value } = event.target;\n    setPatient({ ...patient, smoke: value });\n  };\n\n  const fileOnchange = (event) => {\n    //Consigue el archivo del form\n    const file = event.target.files[0];\n    //para enviar el objeto y añadir la imagen\n    const uploadData = new FormData();\n    uploadData.append('photo', file);\n    axios\n      .post(`${process.env.REACT_APP_API_URI}/dietitian/upload/`, uploadData, {\n        withCredentials: true,\n      })\n      .then((response) => {\n        setPatient({ ...patient, imageUrl: response.data.secure_url, disable: false });\n      })\n      .catch((error) => console.log(error));\n  };\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n    try {\n      const newPatient = await dietitianService.createPatient(patient);\n      const dietitianUpdate = await dietitianService.add(newPatient._id);\n      setDietitian(dietitianUpdate);\n      redirToProfile(newPatient._id);\n    } catch (err) {\n      console.log(err);\n    }\n    setPatient({\n      imageUrl: '',\n      name: '',\n      lastName: '',\n      email: '',\n      gender: '',\n      age: 0,\n      weight: 0,\n      height: 0,\n      hipPerimeter: 0,\n      neckPerimeter: 0,\n      objectives: '',\n      foodAllergies: '',\n      smoke: '',\n    });\n\n    // if (\n    //   // imageUrl.trim() === null ||\n    //   !name ||\n    //   !lastName ||\n    //   !email ||\n    //   !gender ||\n    //   !age ||\n    //   !weight ||\n    //   !height ||\n    //   !hipPerimeter ||\n    //   !neckPerimeter ||\n    //   !objectives ||\n    //   !foodAllergies ||\n    //   !smoke\n  };\n  const redirToProfile = (patientId) => {\n    history.push(`/PatientProfile/${patientId}`);\n  };\n\n  //INTRODUCIR FOTO DE PERFIL DEL PACIENTE\n  // const fileOnchange = async (event) => {\n  //   //Consigue el archivo del form\n  //   const file = event.target.files[0];\n  //   //para enviar el objeto y añadir la imagen\n  //   const uploadData = new FormData();\n  //   uploadData.append('photo', file);\n  //   try {\n  //     const sendFile = await cloudinaryService.fileUpload(uploadData);\n  //     setPatient({ ...patient, imageUrl: sendFile });\n  //     console.log(patient);\n  //   } catch (error) {\n  //     console.log('error', error);\n  //   }\n  // };\n\n  return (\n    <div className=\"container\">\n      <h1>Registrer a Patient</h1>\n\n      <form onSubmit={handleFormSubmit}>\n        {error && <Error>Todos los cambios son obligatorios</Error>}\n\n        {/* <label>profile image</label> */}\n        <ImgUser src={imageUrl} alt=\"patient Img\" />\n\n        <div className=\"input-group mb-3\">\n          <div className=\"input-group-prepend\">\n            <span className=\"input-group-text\">Upload</span>\n          </div>\n          <div className=\"custom-file\">\n            <input\n              onChange={fileOnchange}\n              type=\"file\"\n              className=\"custom-file-input\"\n              id=\"inputGroupFile01\"\n            />\n            <label className=\"custom-file-label\">Choose user photo</label>\n          </div>\n        </div>\n\n        <label>Género:</label>\n        <div className=\"form-check form-check-inline\">\n          <input\n            className=\"form-check-input\"\n            type=\"radio\"\n            name=\"gender\"\n            value=\"masculino\"\n            checked={gender === 'masculino'}\n            onChange={handleGenderChange}\n          />\n          <label className=\"form-check-label\">Masculino</label>\n        </div>\n        <div className=\"form-check form-check-inline\">\n          <input\n            className=\"form-check-input\"\n            type=\"radio\"\n            name=\"gender\"\n            value=\"femenino\"\n            checked={gender === 'femenino'}\n            onChange={handleGenderChange}\n          />\n          <label className=\"form-check-label\">Femenino</label>\n        </div>\n\n        <div className=\"form-row\">\n          <div className=\"col\">\n            <label>Name:</label>\n            <input\n              type=\"text\"\n              className=\"form-control\"\n              placeholder=\"First name\"\n              name=\"name\"\n              value={name}\n              onChange={handleChange}\n              required\n            />\n          </div>\n          <div className=\"col\">\n            <label>Last name:</label>\n            <input\n              type=\"text\"\n              className=\"form-control\"\n              placeholder=\"Last name\"\n              name=\"lastName\"\n              value={lastName}\n              onChange={handleChange}\n            />\n          </div>\n        </div>\n\n        <div className=\"form-group\">\n          <label>E-mail</label>\n          <input\n            type=\"email\"\n            name=\"email\"\n            value={email}\n            onChange={handleChange}\n            placeholder=\"exemple@email.com\"\n            required\n            className=\"form-control\"\n          />\n        </div>\n        <div className=\"form-group\">\n          <label>Edad</label>\n          <input\n            type=\"text\"\n            name=\"age\"\n            value={age}\n            onChange={handleChange}\n            className=\"form-control\"\n            placeholder=\"Edad\"\n          />\n        </div>\n\n        <div className=\"form-group\">\n          <label>Peso</label>\n          <input\n            type=\"number\"\n            name=\"weight\"\n            value={weight}\n            onChange={handleChange}\n            placeholder=\"80 Kg\"\n            className=\"form-control\"\n          />\n        </div>\n        <div className=\"form-group\">\n          <label>Altura</label>\n          <input\n            type=\"number\"\n            name=\"height\"\n            value={height}\n            onChange={handleChange}\n            placeholder=\"175 cm\"\n            className=\"form-control\"\n          />\n        </div>\n        <div className=\"form-group\">\n          <label>Perimetro del cuello</label>\n          <input\n            type=\"text\"\n            name=\"neckPerimeter\"\n            value={neckPerimeter}\n            onChange={handleChange}\n            placeholder=\"37cm\"\n            className=\"form-control\"\n          />\n        </div>\n        <div className=\"form-group\">\n          <label>Perimetro de la cintura</label>\n          <input\n            type=\"text\"\n            name=\"hipPerimeter\"\n            value={hipPerimeter}\n            onChange={handleChange}\n            placeholder=\"85cm\"\n            className=\"form-control\"\n          />\n        </div>\n        <div className=\"form-group\">\n          <label>¿Alergias alimenticias?</label>\n          <input\n            type=\"text\"\n            name=\"foodAllergies\"\n            value={foodAllergies}\n            onChange={handleChange}\n            className=\"form-control\"\n          />\n        </div>\n\n        <div>\n          <label>¿Fumador?</label>\n          <div className=\"form-check form-check-inline\">\n            <input\n              className=\"form-check-input\"\n              type=\"radio\"\n              name=\"smoke\"\n              value=\"no\"\n              checked={smoke === 'no'}\n              onChange={handleSmokeChange}\n            />\n            <label className=\"form-check-label\">No</label>\n          </div>\n          <div className=\"form-check form-check-inline\">\n            <input\n              className=\"form-check-input\"\n              type=\"radio\"\n              name=\"smoke\"\n              value=\"yes\"\n              checked={smoke === 'yes'}\n              onChange={handleSmokeChange}\n            />\n            <label className=\"form-check-label\">Sí</label>\n          </div>\n        </div>\n\n        <div className=\"form-group\">\n          <label>Objetivos del usuario</label>\n          <input\n            type=\"text\"\n            name=\"objectives\"\n            value={objectives}\n            onChange={handleChange}\n            placeholder=\"Definir musculatura\"\n            className=\"form-control\"\n          />\n        </div>\n\n        <input\n          className=\"btn btn-primary btn-lg btn-block\"\n          type=\"submit\"\n          value=\"Registrar paciente\"\n        />\n      </form>\n      <div>\n        <Link className=\"btn btn-secondary btn-sm\" to={'/'}>\n          Home\n        </Link>\n      </div>\n    </div>\n  );\n};\n\nexport default withAuth(PatientForm);\n",["116","117"],"/Users/jorditorrasmellado/Documents/Ironhack/Proyectos/ProjectM3-Dietum/Dietum-FrontEnd/src/components/AnonRoute.js",[],"/Users/jorditorrasmellado/Documents/Ironhack/Proyectos/ProjectM3-Dietum/Dietum-FrontEnd/src/pages/EditDietitian.js",[],["118","119"],"/Users/jorditorrasmellado/Documents/Ironhack/Proyectos/ProjectM3-Dietum/Dietum-FrontEnd/src/index.js",[],"/Users/jorditorrasmellado/Documents/Ironhack/Proyectos/ProjectM3-Dietum/Dietum-FrontEnd/src/lib/AuthProvider.js",[],"/Users/jorditorrasmellado/Documents/Ironhack/Proyectos/ProjectM3-Dietum/Dietum-FrontEnd/src/pages/Private.js",[],"/Users/jorditorrasmellado/Documents/Ironhack/Proyectos/ProjectM3-Dietum/Dietum-FrontEnd/src/components/PrivateRoute.js",[],"/Users/jorditorrasmellado/Documents/Ironhack/Proyectos/ProjectM3-Dietum/Dietum-FrontEnd/src/lib/dietitian-service.js",[],"/Users/jorditorrasmellado/Documents/Ironhack/Proyectos/ProjectM3-Dietum/Dietum-FrontEnd/src/pages/Signup.js",[],"/Users/jorditorrasmellado/Documents/Ironhack/Proyectos/ProjectM3-Dietum/Dietum-FrontEnd/src/components/Sidebar.js",[],"/Users/jorditorrasmellado/Documents/Ironhack/Proyectos/ProjectM3-Dietum/Dietum-FrontEnd/src/components/Navbar.js",["120","121","122"],"import React, { useState } from 'react';\nimport